{
  "headers": [
    {
      "source": "/.*\\.(?:css|js)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/.*\\.(?:woff2|woff|ttf|otf|eot|png|jpg|jpeg|gif|webp|svg|ico|mp4|webm)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
